<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.homegym">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/title_activity_login2"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RecItemActivity"
            android:label="RecItemActivity 액티비티" />
        <activity
            android:name=".LrecItemActivity"
            android:label="LrecItemActivity 액티비티" />
        <activity
            android:name=".UpperActivity"
            android:label="UpperActivity 액티비티" />
        <activity
            android:name=".LowerActivity"
            android:label="LowerActivity 액티비티" />
        <activity
            android:name=".DietActivity"
            android:label="DietActivity 액티비티" />
    </application>

</manifest>